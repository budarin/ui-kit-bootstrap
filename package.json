        "@types/node": "^10.9.4",
        "@types/react": "^16.4.13",
        "@types/react-dom": "^16.0.7",
        "jest": "^23.5.0",
        "lerna": "^3.2.1",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "rimraf": "^2.6.2",
        "storybook": "^1.0.0",
