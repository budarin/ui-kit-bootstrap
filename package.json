{
    "name": "ui-kit-bootstrap",
    "version": "1.0.0",
    "description": "ui-kit bootstrap",
    "scripts": {
        "clean": "lerna clean --yes && lerna exec rimraf ./lib  --parallel",
        "build": " lerna exec rimraf ./lib  --parallel && tsc -b packages",
        "bootstrap": "lerna bootstrap --hoist",
        "prepublish": "npm run build",
        "publish": "lerna publish",
        "lerna:updated": "lerna updated",
        "upgrade": "ncu --upgradeAll && npm install",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
